<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="RestAttributeMotherGrid">
        <property name="idAttributeMotherGrid" type="int"/>
        <property name="name" type="string"/>
        <property name="amgKey" type="string"/>
        <property name="values" singular="value" type="AttributeMotherGridValueStorage[]"/>
    </transfer>

    <transfer name="RestAttributeGridProductAbstract">
        <property name="idProductAbstract" type="int"/>
        <property name="idAttributeGridGroup" type="int"/>
        <property name="attributeGridGroup" type="string"/>
        <property name="countryValues" singular="countryValue" type="AttributeGridCountryStorage[]"/>
    </transfer>

</transfers>
