<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="AttributeMotherGrid">
        <property name="idAttributeMotherGrid" type="int"/>
        <property name="name" type="string"/>
    </transfer>

    <transfer name="AttributeMotherGridKey">
        <property name="idAttributeMotherGridKey" type="int"/>
        <property name="fkAttributeMotherGrid" type="int"/>
        <property name="key" type="string"/>
    </transfer>

    <transfer name="AttributeMotherGridValue">
        <property name="idAttributeMotherGridValue" type="int"/>
        <property name="fkAttributeMotherGridKey" type="int"/>
        <property name="value" type="string"/>
    </transfer>

    <transfer name="AttributeGridGroup">
        <property name="idAttributeGridGroup" type="int"/>
        <property name="group" type="string"/>
    </transfer>

    <transfer name="AttributeGridValue">
        <property name="idAttributeGridValue" type="int"/>
        <property name="fkAttributeMotherGridKey" type="int"/>
        <property name="fkAttributeGridGroup" type="int"/>
        <property name="value" type="string"/>
    </transfer>

</transfers>
